<?xml version="1.0" encoding="UTF-8"?>

<!-- **************** PR **************** -->
<!-- a > a, in -->

<rules>
  <rule weight="0.6">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <match tags="det./"/>
    <or>
      <match lemma="nord"/>
      <match lemma="nord-est"/>
      <match lemma="nord-oest"/>
      <match lemma="sud"/>
      <match lemma="sud-est"/>
      <match lemma="sud-oest"/>
      <match lemma="est"/>
      <match lemma="oest"/>

      <match lemma="parlament"/>
      <match lemma="congrés"/>
      <match lemma="senat"/>
      <match lemma="assamblea"/>
      <match lemma="conferència"/>

      <match lemma="escrit"/>
      <match lemma="text"/>
      <match lemma="llibre"/>
      <match lemma="revista"/>
      <match lemma="publicació"/>
      <match lemma="article"/>

      <match lemma="hora"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or><match lemma="estar"/><match lemma="ser"/><match lemma="quedar"/></or>
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or><match lemma="estar"/><match lemma="ser"/><match lemma="quedar"/></or>
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
  </rule>

<!-- davant > in antis, ante -->

  <rule weight="0.6">
    <match lemma="davant" tags="pr"><select lemma="in antis" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="davant" tags="pr"><select lemma="ante" tags="pr"/></match>
    <match tags="det./"/>
    <or>
      <match lemma="policia" tags="n.*"/>
      <match lemma="mosso" tags="n.*"/>
      <match lemma="jutge" tags="n.*"/>
      <match lemma="magistrat" tags="n.*"/>
      <match lemma="tribunal" tags="n.*"/>
    </or>
  </rule>

<!-- de > de, dae -->
<!-- per > pro, dae -->
<!-- pp dae -->

  <rule weight="0.6">
    <match lemma="de" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>
  <rule weight="0.6">
    <match lemma="per" tags="pr"><select lemma="pro" tags="pr"/></match>
  </rule>

  <rule weight="1.0"> <!-- complement agent introduït per "dae" en sard -->
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match lemma="per" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match tags="adv"/>
     <match lemma="per" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.2">
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match lemma="per" tags="pr"><select lemma="pro" tags="pr"/></match>
     <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>
  <rule weight="1.2">
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match tags="adv"/>
     <match lemma="per" tags="pr"><select lemma="pro" tags="pr"/></match>
     <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>
  <rule weight="1.0">
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match lemma="de" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
     <match tags="adv"/>
     <match lemma="de" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>

  <rule weight="1.0">
     <or>
       <match lemma="venir" tags="vblex.*"/>
       <match lemma="arribar" tags="vblex.*"/>
       <match lemma="eliminar" tags="vblex.*"/>
       <match lemma="treure" tags="vblex.*"/>
       <match lemma="esborrar" tags="vblex.*"/>
     </or>
     <match lemma="de" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>

  <rule weight="1.0">
     <match lemma="de" tags="pr"><select lemma="dae" tags="pr"/></match>
     <match lemma="un" tags="det.*"/>
     <or><match lemma="costat" tags="n.*"/><match lemma="banda" tags="n.*"/></or>
  </rule>

  <rule weight="1.0">
     <match lemma="per" tags="pr"><select lemma="dae" tags="pr"/></match>
     <match lemma="un" tags="det.*"/>
     <or><match lemma="costat" tags="n.*"/><match lemma="banda" tags="n.*"/></or>
  </rule>

<!-- al llarg de > durante, a largu de -->

  <rule weight="0.6">
    <match lemma="al llarg de" tags="pr"><select lemma="durante" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="al llarg de" tags="pr"><select lemma="al llarg de" tags="pr"/></match>
    <match tags="np.*"/>
  </rule>

<!-- segons (pr) > segundu, a parrere de -->

  <rule weight="0.6">
     <match lemma="segons" tags="pr"><select lemma="segundu"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="segons" tags="pr"><select lemma="a pàrrere de"/></match>
     <match tags="np.*"/>
  </rule>


<!-- **************** VBLEX **************** -->

<!-- hi ha > b'at -->

  <rule weight="1.0">
     <match lemma="hi" tags="prn.pro.adv"><select lemma="bi" tags="prn.*"/></match>
     <match tags="vbhaver.*"/>
  </rule>
  <rule weight="1.2">
     <match lemma="hi" tags="prn.pro.adv"><select lemma="nche" tags="prn.*"/></match>
     <match tags="vbhaver.*"/>
     <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/><match tags="vbser.pp.*"/></or>
  </rule>
  <rule weight="0.6">
     <match lemma="hi" tags="prn.pro.adv"><select lemma="nche" tags="prn.*"/></match>
  </rule>

<!-- estar > èssere, istare -->

  <rule weight="0.6">
     <match lemma="estar" tags="vblex.*"><select lemma="èssere"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="estar" tags="vblex.*"><select lemma="istare"/></match>
    <or><match lemma="sempre"/><match lemma="tot" tags="predet.*"/></or>
  </rule>

<!-- ensenyar > insegnare, mustrare -->

  <rule weight="0.6">
     <match lemma="ensenyar" tags="vblex.*"><select lemma="insingiare"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="ensenyar" tags="vblex.*"><select lemma="mustrare"/></match>
     <match lemma="a" tags="pr"/>
  </rule>

<!-- permetre > permìtere, permìtere de -->

  <rule weight="0.6">
     <match lemma="permetre" tags="vblex.*"><select lemma="permìtere"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="permetre" tags="vblex.*"><select lemma="permìtere# de"/></match>
     <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>

<!-- demanar > pregontare, pedire -->

  <rule weight="0.6">
     <match lemma="demanar" tags="vblex.*"><select lemma="pregontare"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match lemma="que"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="adj.*"/>
     <match lemma="que"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match tags="det.*"/>
     <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="adj.*"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="adj.*"/>
     <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="de" tags="pr"/>
     <match tags="vblex.inf"/>
     <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match lemma="de" tags="pr"/>
     <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
     <match lemma="demanar" tags="vblex.*"><select lemma="pedire"/></match>
     <match lemma="a" tags="pr"/>
     <match tags="det.*"/>
     <match tags="n.*"/>
     <match tags="adj.*"/>
     <match lemma="de" tags="pr"/>
     <match tags="vblex.inf"/>
  </rule>

<!-- arrencar, arrancar > irraighinare, istratzare, pònnere# in motu (la primera traducción es para "sradicare"; la segunda para "strappare" la tercera es "mettere in moto" (arrancar el coche)) -->

  <rule weight="0.6">
     <match lemma="arrencar" tags="vblex.*"><select lemma="irraighinare"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="arrencar" tags="vblex.*"><select lemma="pònnere# in motu"/></match>
     <match tags="det.*"/>
     <or><match lemma="vehicle"/><match lemma="cotxe"/><match lemma="moto"/></or>
  </rule>

  <rule weight="0.6">
     <match lemma="arrancar" tags="vblex.*"><select lemma="irraighinare"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="arrancar" tags="vblex.*"><select lemma="pònnere# in motu"/></match>
     <match tags="det.*"/>
     <or><match lemma="vehicle"/><match lemma="cotxe"/><match lemma="moto"/></or>
  </rule>

<!-- estimar > amare, istimare -->

  <rule weight="0.6">
     <match lemma="estimar" tags="vblex.*"><select lemma="amare"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="estimar" tags="vblex.*"><select lemma="istimare"/></match>
     <match lemma="en" tags="pr"/>
  </rule>
  <rule weight="1.0">
     <match lemma="estimar" tags="vblex.*"><select lemma="istimare"/></match>
     <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
     <match lemma="es" tags="prn.pro.ref.*"/>
     <match lemma="estimar" tags="vblex.*"><select lemma="istimare"/></match>
  </rule>

<!-- tractar de > tratare de, proare de -->

  <rule weight="0.6">
     <match lemma="tractar# de" tags="vblex.*"><select lemma="tratare# de"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="tractar# de" tags="vblex.*"><select lemma="proare# de"/></match>
     <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>

<!-- guanyar > bìnchere, balangiare -->

  <rule weight="0.6">
     <match lemma="guanyar" tags="vblex.*"><select lemma="bìnchere"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="guanyar" tags="vblex.*"><select lemma="balangiare"/></match>
     <match lemma="diner" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="guanyar" tags="vblex.*"><select lemma="balangiare"/></match>
     <match tags="adj.*"/>
     <match lemma="diner" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="guanyar" tags="vblex.*"><select lemma="balangiare"/></match>
     <or><match tags="num.*"/><match tags="adj.*"/></or>
     <or><match lemma="euro" tags="n.*"/><match lemma="pesseta" tags="n.*"/><match lemma="lira" tags="n.*"/><match lemma="franc" tags="n.*"/><match lemma="marc" tags="n.*"/><match lemma="dòlar" tags="n.*"/></or>
  </rule>


<!-- COMPARACIONS -->
<!-- que > chi, de (comparacions) -->

