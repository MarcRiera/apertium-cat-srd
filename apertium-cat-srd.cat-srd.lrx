  <rule weight="1.0">
     <or><match lemma="pitjor" tags="adv"/><match lemma="millor" tags="adv"/></or>
     <match lemma="que" tags="cnjsub"><select lemma="de"/></match>
     <match tags="prn.tn.pos.*"/>
  </rule>

  <rule weight="1.0">
     <or><match lemma="més" tags="adv"/><match lemma="menys" tags="adv"/></or>
     <or><match tags="adj.*"/><match tags="adv"/></or>
     <match lemma="que" tags="cnjsub"><select lemma="de"/></match>
     <or><match tags="prn.tn.p1.*"/><match tags="prn.tn.p2.*"/><match tags="prn.tn.p3.*"/></or>
  </rule>
  <rule weight="1.2">
